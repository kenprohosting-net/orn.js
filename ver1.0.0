  .hy-ease-in { transition-timing-function: cubic-bezier(0.4, 0, 1, 1); }
  .hy-ease-out { transition-timing-function: cubic-bezier(0, 0, 0.2, 1); }
  .hy-ease-in-out { transition-timing-function: cubic-bezier(0.4, 0, 0.2, 1); }
  
  .hy-cursor-auto { cursor: auto; }
