  .hy-cursor-not-allowed { cursor: not-allowed; }
  
  .hy-select-none { user-select: none; }
  .hy-select-text { user-select: text; }
  .hy-select-all { user-select: all; }
  .hy-select-auto { user-select: auto; }
  
  .hy-pointer-events-none { pointer-events: none; }
  .hy-pointer-events-auto { pointer-events: auto; }
  
  .hy-sr-only {
