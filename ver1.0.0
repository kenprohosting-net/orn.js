// JavaScript Implementation
(function(global) {
    "use strict";

    // =========================================================================
    // 0. UNIVERSAL ENVIRONMENT DETECTION
    // =========================================================================
    const ENV = {
        isBrowser: typeof window !== 'undefined',
        isNode: typeof process !== 'undefined' && process.versions && process.versions.node,
        isWebWorker: typeof self !== 'undefined' && typeof window === 'undefined',
        isServiceWorker: typeof importScripts === 'function'
    };

    // =========================================================================
    // 1. CSS INJECTION
    // =========================================================================
    const injectCSS = () => {
        if (ENV.isBrowser && !document.getElementById('orn-css')) {
            const style = document.createElement('style');
            style.id = 'orn-css';
            style.textContent = ORN_CSS_FRAMEWORK;
            document.head.appendChild(style);
        }
    };

    // =========================================================================
    // 2. CORE ORN OBJECT & ENGINE REGISTRIES
    // =========================================================================
    
    const Orn = {
        version: '1.0.0',
        env: ENV,
        
        // --- Centralized Stores ---
        store: {
            state: {},
            signals: new Map(),
            components: new Map(),
            routes: new Map()
        },

        // --- Engine Namespaces ---
        hy: {}, // Hypa engine (hy- attributes)
        rv: {}, // ReacTV engine (rv- attributes)
        un: {}, // Uno engine (un- attributes)

        // --- Initialization ---
        init(options = {}) {
            console.log(`%c ORN.JS v${this.version} `, "background: #6200ea; color: white; padding: 4px 8px; border-radius: 4px; font-weight: bold;");
            
            // 1. Inject Unified CSS
            injectCSS();

            // 2. Initialize Subsystems
            this.hy.setup();
            this.rv.setup();
            this.un.setup();

            // 3. Start DOM Scanning (Browser only)
            if (ENV.isBrowser) {
                if (document.readyState === 'loading') {
                    document.addEventListener('DOMContentLoaded', () => this.scanDOM());
                } else {
                    this.scanDOM();
                }
            }
        },

        // --- Unified DOM Scanner ---
        scanDOM() {
            if (!ENV.isBrowser) return;
            
            // 1. Scan for hy-comp, rv-comp, un-comp components
            document.querySelectorAll('[hy-comp]').forEach(el => {
                const name = el.getAttribute('hy-comp');
                const propsStr = el.getAttribute('hy-props');
                let props = {};
                try { if(propsStr) props = JSON.parse(propsStr); } catch(e) { console.error("Orn Props Error", e); }
                this.hy.mount(name, el, props);
            });

            document.querySelectorAll('[rv-comp]').forEach(el => {
                const name = el.getAttribute('rv-comp');
                const propsStr = el.getAttribute('rv-props');
                let props = {};
                try { if(propsStr) props = JSON.parse(propsStr); } catch(e) { console.error("Orn Props Error", e); }
                this.rv.mount(name, el, props);
            });

            document.querySelectorAll('[un-comp]').forEach(el => {
                const name = el.getAttribute('un-comp');
                const propsStr = el.getAttribute('un-props');
                let props = {};
                try { if(propsStr) props = JSON.parse(propsStr); } catch(e) { console.error("Orn Props Error", e); }
                this.un.mount(name, el, props);
            });

            // 2. Scan for declarative bindings (Hypa style)
            this.hy.scanBindings(document.body);
        }
    };

    // =========================================================================
    // 3. ORN.HY (HYPA.JS) - REACTIVITY & NEURAL MORPHING
    // =========================================================================
    
    Orn.hy = {
        components: new Map(),
        signals: new Map(),
        effects: new Map(),
        activeEffect: null,

        setup() {
            // Setup Hypa internals
        },

        // --- Core Reactivity ---
        signal(initialValue) {
            let value = initialValue;
            const subs = new Set();
            const id = 'hy-sig-' + Math.random().toString(36).substr(2, 9);
            
            const read = () => {
                if (Orn.hy.activeEffect) subs.add(Orn.hy.activeEffect);
                return value;
            };
            const write = (nextValue) => {
                if (value !== nextValue) {
                    value = nextValue;
                    subs.forEach(fn => fn());
                }
                return value;
            };
            return [read, write, id];
        },

        effect(fn) {
            const run = () => {
                Orn.hy.activeEffect = run;
                try { fn(); } finally { Orn.hy.activeEffect = null; }
            };
            run();
        },

        // --- DOM Logic ---
        scanBindings(root) {
            // hy-text
            root.querySelectorAll('[hy-text]').forEach(el => {
                const expr = el.getAttribute('hy-text');
                // Simple state resolution logic placeholder
                // In full implementation, resolve expr against global state or local scope
                el.textContent = expr; 
            });
            
            // hy-bind (Two-way)
            root.querySelectorAll('[hy-bind]').forEach(el => {
                const binding = el.getAttribute('hy-bind');
                el.addEventListener('input', (e) => {
                    // Update signal based on binding path
                    console.log('Orn.hy binding update:', binding, e.target.value);
                });
            });

            // hy-click (Event)
            root.querySelectorAll('[hy-on\\:click]').forEach(el => {
                const handler = el.getAttribute('hy-on:click');
                el.addEventListener('click', () => {
                    console.log('Orn.hy click:', handler);
                });
            });
        },

        component(name, template) {
            this.components.set(name, { template });
        },

        mount(name, el, props) {
            const comp = this.components.get(name);
            if (comp) {
                // Simple template interpolation for hy- components
                let html = comp.template;
                // Basic interpolation for props {{prop}}
                Object.keys(props).forEach(k => {
                    html = html.replace(new RegExp(`{{${k}}}`, 'g'), props[k]);
                });
                el.innerHTML = html;
                this.scanBindings(el);
            }
        }
    };

    // =========================================================================
    // 4. ORN.RV (REACTV.JS) - FIBER RECONCILIATION & COMPAT
    // =========================================================================

    Orn.rv = {
        components: {},
        hooks: [],
        hookIndex: 0,
        
        setup() { 
            this.hooks = [];
            this.hookIndex = 0;
        },

        // --- Virtual DOM ---
        createElement(type, props, ...children) {
            return {
                type,
                props: { ...props, children: children.flat() },
                key: props?.key || null
            };
        },

        // --- Hooks Implementation ---
        useState(initial) {
            const idx = Orn.rv.hookIndex++;
            const hooks = Orn.rv.hooks;
            if (hooks[idx] === undefined) hooks[idx] = initial;
            const setState = (newVal) => {
                hooks[idx] = typeof newVal === 'function' ? newVal(hooks[idx]) : newVal;
                Orn.rv.requestRender();
            };
            return [hooks[idx], setState];
        },

        useEffect(callback, deps) {
            const idx = Orn.rv.hookIndex++;
            const hasChanged = deps ? !Orn.rv.hooks[idx] || deps.some((d, i) => d !== Orn.rv.hooks[idx][i]) : true;
            if (hasChanged) {
                Orn.rv.hooks[idx] = deps;
                setTimeout(callback, 0);
            }
        },

        // --- Rendering ---
        render(vnode, container) {
            if (typeof vnode.type === 'function') {
                // Functional Component
                Orn.rv.hookIndex = 0; 
                const result = vnode.type(vnode.props);
                container.innerHTML = ''; 
                if (typeof result === 'string') container.textContent = result;
                else if (result.type) container.appendChild(this._createDOM(result));
            }
        },

        _createDOM(vnode) {
            if (typeof vnode === 'string') return document.createTextNode(vnode);
            const el = document.createElement(vnode.type);
            Object.keys(vnode.props || {}).forEach(k => {
                if (k === 'children') return;
                if (k.startsWith('on')) el.addEventListener(k.slice(2).toLowerCase(), vnode.props[k]);
                else el.setAttribute(k, vnode.props[k]);
            });
            (vnode.props.children || []).forEach(child => el.appendChild(this._createDOM(child)));
            return el;
        },

        requestRender() {
            if (ENV.isBrowser) requestAnimationFrame(Orn.scanDOM.bind(Orn));
        },

        mount(name, el, props) {
            const Comp = this.components[name];
            if (Comp) this.render(this.createElement(Comp, props), el);
        }
    };

    // React Compatibility Aliases
    Orn.React = {
        createElement: Orn.rv.createElement,
        useState: Orn.rv.useState,
        useEffect: Orn.rv.useEffect,
        Component: class { constructor(p){this.props=p;} render(){} }
    };

    // =========================================================================
    // 5. ORN.UN (UNO.JS) - ENTERPRISE UTILS, AI & SERVER
    // =========================================================================

    Orn.un = {
        components: {},
        
        setup() {
            // Init AI, DB, Auth subsystems
        },

        // --- AI Subsystem ---
        ai: {
            vectorDB: [],
            push(data) { 
                this.vectorDB.push({ id: Math.random().toString(36), data, vec: [] }); 
                return true;
            },
            query: async (prompt) => {
                console.log(`[Orn UN] Querying RAG for: "${prompt}"`);
                return { answer: "Simulated AI Response", confidence: 0.98 };
            }
        },

        // --- Full Stack Utilities ---
        http: {
            get: async (url) => fetch(url).then(r => r.json()),
            post: async (url, data) => fetch(url, { method:'POST', body: JSON.stringify(data) }).then(r => r.json())
        },

        form: {
            create(schema) {
                return {
                    values: {},
                    errors: {},
                    submit: (fn) => fn(this.values)
                };
            }
        },

        mount(name, el, props) {
            const def = this.components[name];
            if (def) {
                // Simple interpolation for un- components
                let html = def.template;
                Object.keys(props).forEach(k => {
                    html = html.replace(new RegExp(`{{${k}}}`, 'g'), props[k]);
                });
                el.innerHTML = html;
            }
        }
    };

    // =========================================================================
    // 6. SERVER-SIDE RUNTIME (Node.js)
    // =========================================================================
    
    if (ENV.isNode) {
        Orn.server = {
            createApp(config) {
                const http = require('http');
                return http.createServer((req, res) => {
                    res.writeHead(200, {'Content-Type': 'text/html'});
                    res.end('Orn Server Active');
                });
            }
        };
    }

    // =========================================================================
    // 7. EXPORTS
    // =========================================================================

    if (typeof module !== 'undefined' && module.exports) {
        module.exports = Orn;
    } else {
        global.Orn = Orn;
        global.orn = Orn; // Alias for lowercase access
        global.React = Orn.React; // React Polyfill
    }

})(typeof window !== 'undefined' ? window : this);
